{
  "cli": {
    "version": ">= 4.1.2",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "test": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "test"
      },
      "channel": "test"
    },
    "stag": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "stag"
      },
      "channel": "stag"
    },
    "prod": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "prod"
      },
      "channel": "prod"
    }
  },
  "submit": {
    "prod": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "6455988705",
        "appleTeamId": "EPATC4S9N2"
      }
    }
  }
}
