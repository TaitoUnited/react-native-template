{
  "name": "taito-react-native-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "start:clean": "expo start --dev-client --clear",
    "android": "npx expo prebuild && npx expo run:android",
    "android:test": "APP_ENV=test npx expo prebuild && npx expo run:android",
    "android:stag": "APP_ENV=stag npx expo prebuild && npx expo run:android",
    "android:prod": "APP_ENV=prod npx expo prebuild && npx expo run:android",
    "ios": "npx expo run:ios",
    "ios:clean": "npx expo run:ios --no-build-cache",
    "ios:test": "APP_ENV=test npx expo run:ios",
    "ios:stag": "APP_ENV=stag npx expo run:ios",
    "ios:prod": "APP_ENV=prod npx expo run:ios",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild:test": "APP_ENV=test npx expo prebuild",
    "prebuild:stag": "APP_ENV=stag npx expo prebuild",
    "prebuild:prod": "APP_ENV=prod npx expo prebuild",
    "prebuild:ci": "EXPO_NO_GIT_STATUS=1 npx expo prebuild --clean --no-install --skip-dependency-update react,react-native",
    "prebuild:ci:test": "APP_ENV=test npm run prebuild:ci",
    "prebuild:ci:stag": "APP_ENV=stag npm run prebuild:ci",
    "prebuild:ci:prod": "APP_ENV=prod npm run prebuild:ci",
    "design-system:sync": "npm run design-system:tokenize && npm run design-system:codegen",
    "design-system:tokenize": "figmage tokenize --env ./config/.env.figma",
    "design-system:codegen": "figmage codegen --env ./config/.env.figma",
    "i18n:extract": "lingui extract",
    "i18n:extract:clean": "lingui extract --clean",
    "i18n:compile": "lingui compile",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package && jetify"
  },
  "dependencies": {
    "@lingui/macro": "3.14.0",
    "@lingui/react": "3.14.0",
    "@react-native-async-storage/async-storage": "1.17.10",
    "@react-navigation/bottom-tabs": "6.4.0",
    "@react-navigation/native": "6.0.13",
    "@react-navigation/stack": "6.3.3",
    "expo": "~46.0.16",
    "expo-device": "~4.3.0",
    "expo-font": "~10.2.1",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "luxon": "3.1.0",
    "make-plural": "7.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-freeze": "1.0.3",
    "react-hook-form": "7.39.1",
    "react-native": "0.69.6",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-localize": "2.2.4",
    "react-native-permissions": "3.6.1",
    "react-native-reanimated": "2.9.1",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2",
    "react-native-svg": "13.4.0",
    "react-native-toast-message": "2.1.5",
    "react-native-web": "0.18.7",
    "stitches-native": "0.3.0",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@lingui/cli": "3.14.0",
    "@taito/eslint-config-react-native": "github:TaitoUnited/taito-eslint-config-react-native",
    "@types/jest": "29.0.0",
    "@types/luxon": "3.0.1",
    "@types/react": "18.0.18",
    "@types/react-native": "0.69.6",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "8.7.0",
    "figmage": "0.9.0",
    "jest": "29.0.2",
    "metro-react-native-babel-preset": "0.70.3",
    "patch-package": "6.4.7",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "resolutions": {
    "@types/react": "18.0.18",
    "@types/react-native": "0.69.6"
  },
  "engines": {
    "npm": ">=7",
    "yarn": "please-use-npm"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintConfig": {
    "extends": "@taito/react-native"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "lingui": {
    "locales": [
      "en",
      "fi"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po"
  },
  "figmage": {
    "outDir": "src/design-system",
    "tokenize": {
      "tokens": [
        {
          "name": "colors",
          "type": "color"
        },
        {
          "name": "typography",
          "type": "text"
        },
        {
          "name": "shadows",
          "type": "drop-shadow"
        },
        {
          "name": "spacing",
          "type": "width",
          "nodeId": "110:694"
        },
        {
          "name": "radii",
          "type": "radius",
          "nodeId": "102:8"
        },
        {
          "name": "icons",
          "type": "svg",
          "nodeId": "102:2"
        },
        {
          "name": "assets",
          "type": "png",
          "nodeId": "643:618"
        }
      ]
    },
    "codegen": {
      "defaults": {
        "filetype": "ts",
        "tokenCase": "camel"
      },
      "assets": {
        "filetype": "png",
        "tokenCase": "kebab"
      }
    }
  }
}
